            <ComboBox x:Name="comboBox_Copy1" Foreground="#FF66CCFF" BorderThickness="0" HorizontalAlignment="Left" Height="38" Margin="511,8.2,0,0" VerticalAlignment="Top" Width="136" Grid.Row="3"/>
            <Rectangle Fill="#FFF7F7F7" HorizontalAlignment="Stretch"  Margin="0" Grid.Row="2" Stroke="Black" VerticalAlignment="Stretch" />
            <Rectangle Fill="#FFD7D7D7" HorizontalAlignment="Left" Height="120" Margin="123,2,0,0" VerticalAlignment="Top" Width="500" RadiusX="40" RadiusY="40" Grid.Row="2"/>

            <Button Content="操作" HorizontalAlignment="Left" Margin="454,24.4,0,0" VerticalAlignment="Top" Width="99" Background="#FF66CCFF" Grid.Row="2" Height="33"/>
            <TextBlock HorizontalAlignment="Left"   Margin="150,24,0,0" TextWrapping="Wrap" Text="智能机器人答辩" VerticalAlignment="Top" FontSize="20" Width="230" Height="23" Grid.Row="2"/>
            <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="15" Width="55"/>
            <TextBlock Margin="485,80.4,0,0" TextWrapping="Wrap" Text="已读" RenderTransformOrigin="0.45,1.906" FontFamily="Cambria" FontSize="20" Foreground="Red" OpacityMask="Red" FontWeight="Bold" HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Row="2" Height="28" Width="48" />
            <TextBlock HorizontalAlignment="Left" Margin="150,52,0,0" TextWrapping="Wrap" Text="智能机器人答辩" VerticalAlignment="Top" FontSize="20" Width="230" Height="23" Grid.Row="2"/>
            <TextBlock HorizontalAlignment="Left" Margin="150,80,0,0" TextWrapping="Wrap" Text="智能机器人答辩" VerticalAlignment="Top" FontSize="20" Width="230" Height="23" Grid.Row="2"/>
            <Rectangle Fill="#FFF7F7F7" HorizontalAlignment="Stretch"  Margin="0" Grid.Row="4" Stroke="Black" VerticalAlignment="Stretch" />


        </Grid>

    </Grid>
