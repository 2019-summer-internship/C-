            <Border BorderBrush="Gray" Margin="123,8.2,347.4,15.6" BorderThickness="1" CornerRadius="14" Padding="0" Background="White" Grid.Row="3">
                <TextBox x:Name="account_Copy1" BorderThickness="0" Background="Transparent" FontSize="18" Padding="8,0,0,0" VerticalAlignment="Center" Margin="123,232,-123.6,-221.8"/>
            </Border>
            <ComboBox x:Name="comboBox_Copy" Foreground="#FF66CCFF" BorderThickness="0" HorizontalAlignment="Left" Height="38" Margin="511,22,0,0" VerticalAlignment="Top" Width="136"/>
